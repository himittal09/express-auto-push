{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": { "*": ["node_modules/h2-auto-push/build/src/index.d.ts/*"] },
    "rootDir": "src",
    "outDir": "build",
    "target": "es2017",
    "lib": [
      "es2017"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "pretty": true,
    "sourceMap": true,
    "strict": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
